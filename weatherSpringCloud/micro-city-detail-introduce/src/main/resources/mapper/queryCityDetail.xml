<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.microcitydetailintroduce.dos.CityDetailDos">
	
	<!-- 根据城市Id查找 -->
	<select id="queryCityDetail" parameterType="java.lang.String" resultType="map">
	SELECT * FROM city_info
		WHERE cityId = #{cityId}
	</select>
	
</mapper>